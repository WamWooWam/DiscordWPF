<Window x:Class="DiscordWPF.Backend.Config.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiscordWPF.Backend.Config"
        mc:Ignorable="d" Loaded="Window_Loaded" Style="{DynamicResource WindowStyle}"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Margin="10">
        <Label>Executable Path</Label>
        <TextBox x:Name="exeTextBox" Margin="0,0,0,5"/>
        <Label>Main Path</Label>
        <TextBox x:Name="pathTextBox" Margin="0,0,0,5"/>
        <Label>Description</Label>
        <TextBox x:Name="descriptionTextBox" Height="100" AcceptsReturn="True" SpellCheck.IsEnabled="True" Margin="0,0,0,5"/>
        <Button Content="Create!" Click="Button_Click"/>
    </StackPanel>
</Window>
